<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_java">

    <fragment
        android:id="@+id/navigation_java"
        android:name="com.programmerassistant.programmerassistant.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_java" />

    <fragment
        android:id="@+id/navigation_js"
        android:name="com.programmerassistant.programmerassistant.ui.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_js" />

    <fragment
        android:id="@+id/navigation_py"
        android:name="com.programmerassistant.programmerassistant.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_py" />


</navigation>